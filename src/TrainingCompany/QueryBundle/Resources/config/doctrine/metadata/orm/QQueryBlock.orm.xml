<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QQueryBlock" table="q_queryblock">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="qid" type="integer" column="qid" nullable="false"/>
    <field name="qno" type="integer" column="qno" nullable="false"/>
    <field name="label" type="string" column="label" length="150" nullable="true"/>
    <field name="qtype" type="integer" column="qtype" nullable="false"/>
  </entity>
</doctrine-mapping>
