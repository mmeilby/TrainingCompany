<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="QSurveys" table="q_surveys">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="pid" type="integer" column="pid" nullable="false"/>
    <field name="sid" type="integer" column="sid" nullable="false"/>
    <field name="state" type="integer" column="state" nullable="false"/>
    <field name="qno" type="integer" column="qno" nullable="false"/>
    <field name="date" type="date" column="date" nullable="true"/>
    <field name="token" type="string" column="token" length="36" nullable="true"/>
  </entity>
</doctrine-mapping>
